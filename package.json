{
  "name": "music-app-host",
  "private": true,
  "version": "1.0.0",
  "description": "Main Music App - Micro Frontend Host Application",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "clean": "rm -rf dist node_modules",
    "install:all": "npm install && cd ../music-library && npm install",
    "dev:all": "concurrently \"npm run dev\" \"cd ../music-library && npm run dev\"",
    "build:all": "npm run build && cd ../music-library && npm run build"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.12",
    "@mui/material": "^5.15.12",
    "@originjs/vite-plugin-federation": "^1.3.5",
    "jwt-decode": "^4.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3"
  },
  "devDependencies": {
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "typescript": "^5.2.2",
    "vite": "^5.1.6"
  },
  "keywords": [
    "react",
    "micro-frontend",
    "module-federation",
    "music-app",
    "authentication",
    "role-based-access"
  ],
  "author": "Your Name",
  "license": "MIT"
}